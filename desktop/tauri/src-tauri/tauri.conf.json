{
    "productName": "qBit Manage",
    "version": "0.1.0",
    "identifier": "com.qbitmanage.desktop",
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devUrl": "http://localhost:8080",
        "frontendDist": null
    },
    "bundle": {
        "category": "Utility",
        "icon": [
            "../../../icons/qbm_logo.icns",
            "../../../icons/qbm_logo.ico",
            "../../../icons/qbm_logo.png"
        ],
        "targets": "all",
        "externalBin": [
            "bin/qbit-manage-windows-amd64.exe",
            "bin/qbit-manage-macos-x86_64",
            "bin/qbit-manage-macos-arm64",
            "bin/qbit-manage-linux-amd64"
        ],
        "linux": {
            "deb": {
                "depends": [
                    "libgtk-3-0",
                    "libayatana-appindicator3-1",
                    "libwebkit2gtk-4.1-0"
                ]
            }
        }
    },
    "app": {
        "windows": [
            {
                "title": "qBit Manage",
                "width": 1100,
                "height": 800,
                "minWidth": 900,
                "minHeight": 600,
                "resizable": true,
                "fullscreen": false,
                "decorations": true,
                "visible": true
            }
        ],
        "trayIcon": {
            "iconPath": "../../../icons/qbm_logo.png",
            "iconAsTemplate": false
        },
        "security": {
            "csp": null
        },
        "withGlobalTauri": false
    }
}
